        "ERC721A/=lib/utility-contracts/lib/ERC721A/contracts/",
        "ds-test/=lib/ds-test/src/",
        "eth-gas-reporter/=node_modules/eth-gas-reporter/",
        "forge-std/=lib/forge-std/src/",
