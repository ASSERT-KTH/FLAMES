{
    "remappings": [
        "@openzeppelin/contracts/=lib/openzeppelin-contracts/contracts/",
        "@tempusfinance/tempus-utils/contracts/=lib/tempus-utils/contracts/",
        "ERC4626/=lib/properties/lib/ERC4626/contracts/",
        "ds-test/=lib/forge-std/lib/ds-test/src/",
        "erc4626-tests/=lib/openzeppelin-contracts/lib/erc4626-tests/",
        "forge-std/=lib/forge-std/src/",
        "openzeppelin-contracts/=lib/openzeppelin-contracts/",
        "openzeppelin/=lib/openzeppelin-contracts/contracts/",
        "properties/=lib/properties/contracts/",
        "solmate/=lib/properties/lib/solmate/src/",
        "tempus-utils/=lib/tempus-utils/contracts/"
    ],
    "optimizer": {
        "enabled": true,
        "runs": 200
    },
    "metadata": {
        "bytecodeHash": "ipfs",
        "appendCBOR": true
    },
    "outputSelection": {
        "*": {
            "*": [
                "evm.bytecode",
                "evm.deployedBytecode",
                "devdoc",
                "userdoc",
                "metadata",
                "abi"
            ]
        }
    },
    "evmVersion": "london",
    "libraries": {}
}