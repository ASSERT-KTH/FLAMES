{
    "remappings": [
        "openzeppelin-contracts/=lib/openzeppelin-contracts/",
        "openzeppelin/=lib/openzeppelin-contracts/contracts/",
        "src/=src/"
    ],
    "optimizer": {
        "enabled": true,
        "runs": 200
    },
    "metadata": {
        "bytecodeHash": "ipfs"
    },
    "outputSelection": {
        "*": {
            "*": [
                "evm.bytecode",
                "evm.deployedBytecode",
                "devdoc",
                "userdoc",
                "metadata",
                "abi"
            ]
        }
    },
    "evmVersion": "london"
}