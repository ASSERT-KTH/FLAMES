        "@erc721a/=lib/ERC721A-Upgradeable/contracts/",
        "@opregister/=lib/operator-filter-registry/src/",
        "@ozc-upgradeable/=lib/openzeppelin-contracts-upgradeable/contracts/access/",
        "@solmate/=lib/solmate/src/",
        "@std/=lib/forge-std/src/",
        "ERC721A-Upgradeable/=lib/ERC721A-Upgradeable/contracts/",
