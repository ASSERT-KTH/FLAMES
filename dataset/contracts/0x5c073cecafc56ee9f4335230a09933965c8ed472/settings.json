{
    "remappings": [
        "@ensdomains/=node_modules/@ensdomains/",
        "@looksrare/=node_modules/@looksrare/",
        "@openzeppelin/=node_modules/@openzeppelin/",
        "eth-gas-reporter/=node_modules/eth-gas-reporter/",
        "hardhat/=node_modules/hardhat/"
    ],
    "optimizer": {
        "enabled": true,
        "runs": 888888
    },
    "metadata": {
        "bytecodeHash": "ipfs",
        "appendCBOR": true
    },
    "outputSelection": {
        "*": {
            "*": [
                "evm.bytecode",
                "evm.deployedBytecode",
                "devdoc",
                "userdoc",
                "metadata",
                "abi"
            ]
        }
    },
    "evmVersion": "london",
    "libraries": {}
}