{
    "remappings": [
        "ds-test/=contracts-lib/forge-std/lib/ds-test/src/",
        "forge-std/=contracts-lib/forge-std/src/",
        "openzeppelin-contracts/=contracts-lib/openzeppelin-contracts/",
        "token/=contracts-lib/token/"
    ],
    "optimizer": {
        "enabled": true,
        "runs": 200
    },
    "metadata": {
        "bytecodeHash": "ipfs",
        "appendCBOR": true
    },
    "outputSelection": {
        "*": {
            "*": [
                "evm.bytecode",
                "evm.deployedBytecode",
                "devdoc",
                "userdoc",
                "metadata",
                "abi"
            ]
        }
    },
    "evmVersion": "paris",
    "libraries": {}
}