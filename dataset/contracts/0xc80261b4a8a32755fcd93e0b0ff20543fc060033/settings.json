{
    "optimizer": {
        "enabled": true,
        "runs": 500,
        "details": {
            "orderLiterals": true,
            "deduplicate": true,
            "cse": true,
            "yul": true,
            "yulDetails": {
                "stackAllocation": true
            }
        }
    },
    "outputSelection": {
        "*": {
            "*": [
                "evm.bytecode",
                "evm.deployedBytecode",
                "devdoc",
                "userdoc",
                "metadata",
                "abi"
            ]
        }
    },
    "libraries": {}
}