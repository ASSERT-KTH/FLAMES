pragma solidity ^0.8.7;

import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";

interface IERC721 {
    function transferFrom(address, address, uint256) external;
}
contract MaterializerBurn is Ownable, ReentrancyGuard {
    event Materialized(address user, uint256[] tokenIds);

    address public MaterializerContract = 0xD7504d848d3b83F0B0D2ef67F1ca14Ec13F05904;
    IERC721 public Materializer = IERC721(MaterializerContract);

    constructor() {
    }

    function burnMaterializer(uint256[] calldata _tokenIds) external nonReentrant {
        for (uint256 i = 0; i < _tokenIds.length; i++) {
            Materializer.transferFrom(msg.sender, 0x000000000000000000000000000000000000dEaD, _tokenIds[i]);
        }
        emit Materialized(msg.sender, _tokenIds);
    }

    function setMaterializerContract(address _contract) external onlyOwner {
        MaterializerContract = _contract;
        Materializer = IERC721(_contract);
    }

    function withdrawMoney() external onlyOwner {
        (bool success, ) = msg.sender.call{value: address(this).balance}("");
        require(success, "Withdraw failed.");
    }
}