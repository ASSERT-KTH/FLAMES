{
    "evmVersion": "istanbul",
    "optimizer": {
        "enabled": true,
        "runs": 1000000,
        "details": {
            "yul": true,
            "deduplicate": true,
            "cse": true,
            "constantOptimizer": true
        }
    },
    "outputSelection": {
        "*": {
            "*": [
                "evm.bytecode",
                "evm.deployedBytecode",
                "devdoc",
                "userdoc",
                "metadata",
                "abi"
            ]
        }
    },
    "metadata": {
        "useLiteralContent": true
    },
    "libraries": {}
}