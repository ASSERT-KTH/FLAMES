pragma solidity ^0.8.14;

import "@openzeppelin/contracts@4.6.0/token/ERC721/ERC721.sol";
import "@openzeppelin/contracts@4.6.0/access/Ownable.sol";
import "@openzeppelin/contracts@4.6.0/token/ERC721/extensions/ERC721URIStorage.sol";

contract OffURIUnchangeable is ERC721URIStorage, Ownable {

    constructor() ERC721("OffURIUnchangeable", "OFFU") {}

    /**
     * @dev
     * - only the address that deployed this contract can mint onlyOwner
     * - Link tokenId to nftMint function execution address (= deployment address) _msgSender()
     * - Set URI during mint _setTokenURI()
     */
    function nftMint(uint256 tokenId, string calldata uri) public onlyOwner {
        _mint(_msgSender(), tokenId);
        _setTokenURI(tokenId, uri);
    }
}